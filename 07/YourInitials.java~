public class LetterToYourself 
{
    public static void main(String[] args) 
    {
              System.out.println("JJJJJ  BBBB   PPPP");
              System.out.println("  J    B   B  P   P");
              System.out.println("  J    B   B  P   P");
              System.out.println("  J    BBBB   PPPP");
              System.out.println("J J    B   B  P ");
              System.out.println("J J    B   B  P ");
              System.out.println(" JJ    BBBB   P");
    }
}
